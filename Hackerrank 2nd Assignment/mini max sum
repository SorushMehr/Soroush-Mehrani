    if len(arr) == 0:
        print("0 0")
    num_sum = 0
    num_max = arr[0]
    num_min = arr[0]
    for i in range(len(arr)):
        num_sum = num_sum + arr[i]
        if arr[i] > num_max:
            num_max = arr[i]
        elif arr[i] < num_min:
            num_min = arr[i]
    print(str(num_sum - num_max) + " " + str(num_sum - num_min))
